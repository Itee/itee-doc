{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "./tests/"
    ],
    "exclude": [
      "./tests/_results"
    ]
  },
  "plugins": [],
  "opts": {
    "encoding": "utf8",
    "template": "./builds",
    "destination": "./tests/_results/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false,
      "useLongnameInNav": true
    },
    "staticFiles": {
      "include": [
        "./builds/statics"
      ],
      "includePattern": "",
      "excludePattern": "",
      "exclude": []
    }
  }
}
